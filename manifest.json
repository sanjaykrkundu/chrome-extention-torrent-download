{
  "manifest_version": 2,

  "name": "Torrent Opener",
  "description": "Quick open torrent from a link",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["contextMenus"]
}